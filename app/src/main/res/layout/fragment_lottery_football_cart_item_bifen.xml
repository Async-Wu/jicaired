<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/parent"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical"
              android:background="@color/football_bg"
>


    <include layout="@layout/item_foot_cart"/>


    <LinearLayout style="@style/Car">
        <TextView
                android:id="@+id/chosedtext"
                style="@style/Item"
                android:background="@drawable/select_press_line"
                android:textColor="@color/white"
                android:lines="1"
                android:scrollbars="none"
                android:ellipsize="end"
                android:text="胜胜  胜平  胜负  平胜  平平  平负  负胜"
        />
        <ImageView
                android:id="@+id/iv_delete"
                style="@style/Delete"
        />
    </LinearLayout>



</LinearLayout>