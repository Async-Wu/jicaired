<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:background="@color/football_bg"
              android:focusable="false"
              android:orientation="vertical"
              android:focusableInTouchMode="false"
>




    <include layout="@layout/item_foot_cart"/>

    <LinearLayout
            android:id="@+id/linearLayout01"
            style="@style/Car"
            android:layout_below="@+id/ln11"
            android:orientation="horizontal">
        <LinearLayout android:layout_width="0dp"
                      android:layout_weight="1"
                      android:layout_height="wrap_content"
                      android:orientation="vertical">

            <LinearLayout android:layout_width="match_parent"
                          android:layout_height="wrap_content"

                          android:orientation="vertical">

                <View style="@style/Line_Item"/>


                <LinearLayout android:layout_width="match_parent"
                              android:layout_height="wrap_content"

                              android:orientation="horizontal">


                    <LinearLayout
                            android:id="@+id/firstlayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                        <View style="@style/Line_V_Item"/>
                        <LinearLayout
                                android:id="@+id/ln1"
                                style="@style/ItemSecond"
                        >

                            <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/football_txt"
                                    android:textSize="@dimen/txt_item_size"/>

                            <TextView
                                    android:id="@+id/btn0"
                                    android:text="20.47"
                                    style="@style/ItemTxtSecond"/>
                        </LinearLayout>

                        <View style="@style/Line_V_Item"/>
                        <LinearLayout
                                android:id="@+id/ln2"
                                style="@style/ItemSecond">

                            <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="1"
                                    android:textColor="@color/football_txt"
                                    android:textSize="@dimen/txt_item_size"/>

                            <TextView
                                    android:id="@+id/btn1"
                                    style="@style/ItemTxtSecond"
                                    android:text="20.47"
                            />
                        </LinearLayout>
                        <View style="@style/Line_V_Item"/>
                        <LinearLayout
                                android:id="@+id/ln3"
                                style="@style/ItemSecond">

                            <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="2"
                                    android:textColor="@color/football_txt"
                                    android:textSize="@dimen/txt_item_size"/>

                            <TextView
                                    android:id="@+id/btn2"
                                    android:text="20.47"
                                    style="@style/ItemTxtSecond"/>
                        </LinearLayout>
                        <View style="@style/Line_V_Item"/>
                        <LinearLayout
                                android:id="@+id/ln4"
                                style="@style/ItemSecond">

                            <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="3"
                                    android:textColor="@color/football_txt"
                                    android:textSize="@dimen/txt_item_size"/>

                            <TextView
                                    android:id="@+id/btn3"
                                    style="@style/ItemTxtSecond"

                                    android:text="120.47"
                            />
                        </LinearLayout>
                        <View style="@style/Line_V_Item"/>
                    </LinearLayout>


                </LinearLayout>

                <View style="@style/Line_Item"/>

                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"


                >
                    <View style="@style/Line_V_Item"/>

                    <LinearLayout
                            android:id="@+id/ln5"
                            style="@style/ItemSecond">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="4"
                                android:textColor="@color/football_txt"
                                android:textSize="@dimen/txt_item_size"/>

                        <TextView
                                android:id="@+id/btn4"
                                style="@style/ItemTxtSecond"
                                android:text="20.47"
                        />
                    </LinearLayout>
                    <View style="@style/Line_V_Item"/>
                    <LinearLayout
                            android:id="@+id/ln6"
                            style="@style/ItemSecond">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="5"
                                android:textColor="@color/football_txt"
                                android:textSize="@dimen/txt_item_size"/>

                        <TextView
                                android:id="@+id/btn5"
                                style="@style/ItemTxtSecond"
                                android:text="20.47"
                        />
                    </LinearLayout>
                    <View style="@style/Line_V_Item"/>
                    <LinearLayout
                            android:id="@+id/ln7"
                            style="@style/ItemSecond">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="6"
                                android:textColor="@color/football_txt"
                                android:textSize="@dimen/txt_item_size"/>

                        <TextView
                                android:id="@+id/btn6"

                                android:text="20.47"
                                style="@style/ItemTxtSecond"/>
                    </LinearLayout>
                    <View style="@style/Line_V_Item"/>
                    <LinearLayout
                            android:id="@+id/ln8"
                            style="@style/ItemSecond">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="7+"
                                android:textColor="@color/football_txt"
                                android:textSize="@dimen/txt_item_size"/>

                        <TextView
                                android:id="@+id/btn7"
                                style="@style/ItemTxtSecond"
                                android:text="12.47"
                        />
                    </LinearLayout>
                    <View style="@style/Line_V_Item"/>
                </LinearLayout>


                <View style="@style/Line_Item"/>

            </LinearLayout>

        </LinearLayout>
        <ImageView
                android:id="@+id/iv_delete"
                style="@style/Delete"/>

    </LinearLayout>

</LinearLayout>